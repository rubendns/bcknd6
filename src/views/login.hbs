<div >
  <h1 >Login</h1>
  <form id="loginForm">
    <label >Email</label>
    <input type="email" name="email" required />
    <label >Password</label>
    <input
      type="password"
      name="password"
      required
    />
    <input
      type="submit"
      value="Login"
    />
  </form>
  <p class="mt-4">Don't you have an account?
    <a href="/users/register" class="text-blue-500">Register here</a></p>
</div>
<script src="/js/login.js"></script>